{
  "extends": "@react-native/typescript-config",
  "include": ["**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx"],
  "exclude": ["**/node_modules", "**/Pods"],
  "compilerOptions": {
    // "checkJs": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@types/*": ["src/types/*"],
      "@types": ["src/types/index.js"],
    }
  }
}
